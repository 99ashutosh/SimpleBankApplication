{
    
    "env": {
        "myDefaultIncludePath": [
            "${workspaceFolder}",
            "(path-to-msys2-location)//mingw64//include//*"
        ]
    },

    "configurations": [
        {
            "name": "Win32 with GTK includes",
            "intelliSenseMode": "windows-gcc-x64",
            "includePath": [
                "(path-to-msys2-location)//mingw64//include//gtk-3.0",
                "(path-to-msys2-location)//mingw64//include//pango-1.0",
                "(path-to-msys2-location)//mingw64//include",
                "(path-to-msys2-location)//mingw64//include//glib-2.0",
                "(path-to-msys2-location)//mingw64//lib//glib-2.0//include",
                "(path-to-msys2-location)//mingw64//include//harfbuzz",
                "(path-to-msys2-location)//mingw64//include//freetype2",
                "(path-to-msys2-location)//mingw64//include//libpng16",
                "(path-to-msys2-location)//mingw64//include//fribidi",
                "(path-to-msys2-location)//mingw64//include//cairo",
                "(path-to-msys2-location)//mingw64//include//lzo",
                "(path-to-msys2-location)//mingw64//include//pixman-1",
                "(path-to-msys2-location)//mingw64//include//gdk-pixbuf-2.0",
                "(path-to-msys2-location)//mingw64//include//atk-1.0"
            ],
            "compilerPath": "(path-to-msys2-location)//mingw64//bin//gcc.exe",
            "cStandard": "gnu17",
            "cppStandard": "gnu++14"
        }
    ],
    "version": 4
}